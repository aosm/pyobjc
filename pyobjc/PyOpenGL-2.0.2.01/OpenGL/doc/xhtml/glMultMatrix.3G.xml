						<mml:mo>(</mml:mo>
						<mml:mi>v</mml:mi>
						<mml:mo>[</mml:mo>
						<mml:mn>0</mml:mn>
						<mml:mo>]</mml:mo>
						<mml:mi>v</mml:mi>
						<mml:mo>[</mml:mo>
						<mml:mn>1</mml:mn>
						<mml:mo>]</mml:mo>
